@model List<Plot>

<h2>Entered Points</h2>
<form asp-action="AddPoint" method="post">
  <label for="X">X:</label>
  <input type="number" name="X" required>
  <label for="Y">Y:</label>
  <input type="number" name="Y" required>
  <button type="submit">Add Point</button>
</form>
<canvas id="plotCanvas" width="400" height="400"></canvas>

<h2>Achieved Plots</h2>